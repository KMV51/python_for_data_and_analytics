   "source": [
    "def factorial (x):\n",
    "    temp = 0\n",
    "    if x > 1:\n",
    "        temp = x * factorial(x-1)\n",
    "    else:\n",
    "        temp = 1;\n",
    "\n",
    "    \n",
    "    return temp;\n",
    "\n"
   ]
